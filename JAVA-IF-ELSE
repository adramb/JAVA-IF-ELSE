public class if_else{
    public static void main (String[]args){
        int a = 9;
        int b = 7;
        
        System.out.println ("a =" + a);
        System.out.println ("b =" + b);
        if (a < b){
            System.out.println ("a lebih kecil dari b");
        } else {
            System.out.println ("a lebih besar dari b");
        }
    }
}
